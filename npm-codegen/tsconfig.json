        "declaration": true,
        "noImplicitAny": true,
        "removeComments": false,
        "sourceMap": true,
        "outDir": "./dist",
        "noLib": false,
        "preserveConstEnums": true,
        "strict": true
    },
    "include": [
        "../src/objects/*"
